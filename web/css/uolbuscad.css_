rchbox{padding-top:20px;width:100%}#bottom-searchbox table{border-collapse:collapse}#bottom-sbtdinput{width:100%}#bottom-q{border:2px solid #a9cb39;font-size:16px;font-weight:bold;line-height:1.2em;padding:4px 0 5px 5px;width:100%}#bottom-sbtdbutton button,#bottom-sbtdbutton input[type="submit"]{background:#8bc53d url("http://r.i.uol.com.br/d/btn-home.jpg") repeat;border-color:#c1d4a7 #a0bb78 #c2d5a8 #9bbc65;border-width:1px 1px 1px 1px;color:#fff;font-size:17px;font-weight:bold;line-height:1.3em;height:33px;margin:0 0 0 16px;padding:0 18px;text-shadow:1px 1px 2px #999;vertical-align:text-bottom}#bottom-com-google{background:url("http://r.i.uol.com.br/d/com_google.gif") no-repeat 0 4px;float:right;height:20px;line-height:15px;margin:0 12px 0 0;padding-top:2px;text-indent:-9999px;width:61px}#extra{border-left:1px solid #eee;float:right;width:266px}#ofertasuol .adstitle{color:#777;font-size:12px;font-weight:normal;margin:0;padding:0;text-align:right}#ofertasuol .adstitle a{color:#777;text-decoration:none}#ofertasuol .adstitle a:hover{text-decoration:underline}#ofertasuol ul{margin:0 0 20px;padding:0}#ofertasuol li:first-child{margin-top:-5px}ul.results li{*display:inline-block}#header input[type="submit"],#footer input[type="submit"]{*padding:0 8px}

