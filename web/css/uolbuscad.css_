enu-principal{margin:0 0 40px;padding:0}#menu-principal li{border-bottom:1px dashed #ccc;font-size:16px;line-height:28px;list-style:none;margin:0 0 1px;padding:2px 0}#menu-principal li:last-child{border-bottom:0}#menu-principal li a{display:block;padding-left:9px}#menu-principal li a:hover{background-color:#f1f1f1}#menu-principal span.current{background:#f1f1f1 url("http://r.i.uol.com.br/d/link-selecionado.jpg") no-repeat 95% 50%;display:block;font-weight:bold;padding-left:9px}#menu dl{border:5px solid #e7e7e7;color:#191919;border-width:5px 0 0;font-weight:bold;margin:10px 0;padding:10px 0 1px}#menu dt,#menu dd{background-position:8px 2px;background-repeat:no-repeat;margin:5px 0;padding:0 0 0 8px}#menu #menu-web-detalhada,#menu #menu-img-size,#menu #menu-uol-sort{border-bottom-width:5px;padding-bottom:10px}#menu dl a,#menu dl a{font-weight:normal;color:#555}#menu dl a:hover{text-decoration:underline}#content{clear:both;font-size:13px}#main{padding:0 278px 0 210px}#m2{float:left;width:100%}#suggestion{color:#c00;font-size:16px;margin-top:8px}#suggestion a{font-weight:bold;font-style:italic}#filters{background-color:#f7f9d2;border:1px solid #e8e8d0;color:#555;font-weight:bold;margin:2px 0 5px;padding:5px 8px;position:relative}#closefilter{background:url("http://r.i.uol.com.br/d/fechar-filtro.jpg") no-repeat scroll 0 0 transparent;height:12px;position:absolute;right:8px;text-indent:-999em;top:7px;width:13px}ul.results{margin:0 0 14px;padding:0}ul.results li,#ofertasuol li{list-style:none;margin:0;padding:6px 4px 7px 8px}ul.results li:first-child{margin-top:5px}#busca-web.noads-adcontainer1 ul.results li:first-child #busca-uol.noads-adcontainer1 ul.results li:first-child{margin-top:2px}ul.results li.sub{margin-left:28px}ul.results dl{margin:0;padding:0}ul.results dt,ul.results dd.titulo,#ofertasuol dt{font-size:16px}ul.results dd,#ofertasuol dd{padding-left:0;margin-left:0}ul.results dd.link a,#ofertasuol dd.link a{color:#008000;text-decoration:none}ul.results dd.link a